<%= form_with(model: renter, local: true) do |form| %>


  <div class="field">
    <%= form.label :apartmentID %>
    <%= form.number_field :apartmentID, id: :renter_apartmentID %>
  </div>

  <div class="field">
    <%= form.label :firstName %>
    <%= form.text_field :firstName, id: :renter_firstName %>
  </div>

  <div class="field">
    <%= form.label :lastName %>
    <%= form.text_field :lastName, id: :renter_lastName %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
